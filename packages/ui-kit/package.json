{
  "name": "@mauntic/ui-kit",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": { "import": "./dist/index.js", "types": "./dist/index.d.ts" },
    "./layouts": { "import": "./dist/layouts/index.js", "types": "./dist/layouts/index.d.ts" },
    "./components": { "import": "./dist/components/index.js", "types": "./dist/components/index.d.ts" },
    "./styles": "./src/styles/tailwind.css"
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "build:css": "mkdir -p dist && tailwindcss -c tailwind.config.ts -i src/styles/tailwind.css -o dist/ui-kit.css --minify"
  },
  "dependencies": {
    "hono": "catalog:"
  },
  "devDependencies": {
    "typescript": "catalog:",
    "tailwindcss": "catalog:"
  }
}
